{
  "name": "nx-base",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nx-base/src",
  "projectType": "application",
  "targets": {
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "options": {
        "engine": "docker",
        "metadata": {
          "labels": [
            "org.opencontainers.image.title=Nx-Base",
            "org.opencontainers.image.description=Nx-Base Image",
            "org.opencontainers.image.vendor=PRB Services LLC"
          ],
          "images": [],
          "no-cache": true,
          "tags": []
        }
      },
      "configurations": {
        "development": {
          "file": "apps/nx-base/Dockerfile.lts",
          "metadata": {
            "labels": [
              "org.opencontainers.image.title=Nx-Base",
              "org.opencontainers.image.description=Nx-Base Image",
              "org.opencontainers.image.vendor=PRB Services LLC"
            ],
            "images": [
              "nx-base"
            ],
            "tags": [
              "type=schedule",
              "type=ref,event=branch",
              "type=ref,event=tag",
              "type=ref,event=pr",
              "type=semver,pattern={{version}}",
              "type=semver,pattern={{major}}.{{minor}}",
              "type=semver,pattern={{major}}",
              "type=sha"
            ]
          }
        },
        "lts": {
          "file": "apps/nx-base/Dockerfile.lts",
          "metadata": {
            "images": [
              "prbservicesllc/nx-base"
            ],
            "tags": [
              "lts"
            ]
          }
        },
        "current": {
          "file": "apps/nx-base/Dockerfile.current",
          "metadata": {
            "images": [
              "prbservicesllc/nx-base"
            ],
            "tags": [
              "current"
            ]
          }
        }
      }
    }
  },
  "tags": []
}
